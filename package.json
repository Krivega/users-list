{
  "name": "users-list",
  "description": "Implementation of an Users List in the infrastructure of react-graphql",
  "version": "0.1.0",
  "keywords": ["reactjs", "graphql", "react-apollo"],
  "contributors": [
    {
      "name": "Krivega Dmitriy",
      "email": "mr.krivega@gmail.com",
      "web": "http://krivega.com/"
    }
  ],
  "demos": ["http://krivega.com/lab/users-list"],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Krivega/users-list.git"
    }
  ],
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.1.0",
    "apollo-link": "^1.0.7",
    "apollo-link-http": "^1.3.2",
    "bem-cn": "^2.1.3",
    "cross-env": "^5.1.2",
    "eventemitter3": "^3.0.0",
    "flow-bin": "^0.61.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "history": "^4.7.2",
    "lint-staged": "^6.0.0",
    "node-sass-chokidar": "0.0.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.2",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-md": "^1.2.9",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-transition-group": "^2.2.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": ["prettier --write", "git add"]
  },
  "scripts": {
    "precommit": "lint-staged",
    "build-css": "node-sass-chokidar src/ -o src/ --include-path=src/ --include-path=node_modules/",
    "watch-css":
      "npm run build-css && node-sass-chokidar src/ -o src/ --include-path=src/ --include-path=node_modules/ --watch --recursive",
    "start-js": "cross-env NODE_PATH=src react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "cross-env NODE_PATH=src react-scripts build",
    "build": "npm-run-all build-css build-js",
    "flow": "flow",
    "test": "cross-env NODE_PATH=src react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "why-did-you-update": "^0.1.0"
  }
}
